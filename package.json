{
    "name": "cubecubed",
    "version": "1.14.0",
    "description": "Play eye-catching math visualization on the web using SVG and Canvas",
    "keywords": [
        "visualization",
        "svg",
        "canvas",
        "math",
        "animation",
        "graphics"
    ],
    "homepage": "https://imaphatduc.github.io/cubecubed/#/",
    "bugs": {
        "url": "https://github.com/imaphatduc/cubecubed/issues"
    },
    "license": "MIT",
    "author": "imaphatduc",
    "files": [
        "dist/**/*",
        "src/**/*",
        "ccw.js",
        "package.json",
        "README.md"
    ],
    "types": "dist/index.d.ts",
    "main": "dist/cubecubed.js",
    "module": "dist/cubecubed.module.js",
    "bin": {
        "cubecubed": "./ccw.js",
        "ccw": "./ccw.js"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/imaphatduc/cubecubed.git"
    },
    "scripts": {
        "dev": "vite",
        "tsc": "tsc",
        "tsc:watch": "tsc --watch",
        "docs": "typedoc --hideInPageTOC --allReflectionsHaveOwnDocument && cp -rf ./README.md ./docs/",
        "build": "vite build && tsc",
        "test": "vitest --coverage"
    },
    "dependencies": {
        "d3": "^7.4.1",
        "inquirer": "^8.2.2",
        "katex": "^0.16.3",
        "tex-to-svg": "^0.2.0",
        "three": "^0.146.0"
    },
    "devDependencies": {
        "@types/d3": "^7.1.0",
        "@types/d3-simple-slider": "^1.5.2",
        "@types/katex": "^0.14.0",
        "@types/tap": "^15.0.5",
        "@types/three": "^0.146.0",
        "@typescript-eslint/eslint-plugin": "^5.42.1",
        "@typescript-eslint/parser": "^5.42.1",
        "@vitest/coverage-c8": "^0.25.2",
        "c8": "^7.12.0",
        "eslint": "^8.12.0",
        "eslint-config-prettier": "^8.5.0",
        "eslint-plugin-prettier": "^4.0.0",
        "jsdom": "^20.0.0",
        "prettier": "^2.6.1",
        "typedoc": "^0.23.21",
        "typedoc-plugin-markdown": "^3.11.11",
        "typescript": "^4.5.4",
        "vite": "^3.2.3",
        "vite-tsconfig-paths": "^3.4.1",
        "vitest": "^0.25.2"
    }
}
